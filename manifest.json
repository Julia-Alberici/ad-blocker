{
    "manifest_version": 2,
    "name": "Linkedin AdBlocker",
    "description": "Blocking ads.",
    "version": "0.0.1",
    "author": "Julia Alberici",
    "browser_action": {
        "default_title": "LinkedIn adBlocker"
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "https://www.linkedin.com/*"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    }
}